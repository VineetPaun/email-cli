EMAIL_ADDRESS=your_email@gmail.com
EMAIL_PASSWORD=your_app_password
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587

# Same subject for every email in a send run.
EMAIL_SUBJECT=Quick question about your engineering team

# Template to use when running `bun dev`: fe | be | fullstack
POSTCLI_ROLE=fullstack

# Defaults for `send-default`
SEND_DELAY=0
SEND_LIMIT=0
SKIP_CONTACTED=true
MUTATE=true

# Optional
FROM_NAME=Your Name
CONTACTS_FILE=contacts.csv
